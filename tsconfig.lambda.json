{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "outDir": "./buildLambda",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "lambda",
    "node_modules"
  ]
}
